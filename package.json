{
	"name": "lets-raycast-presentation",
	"version": "0.0.0",
	"private": true,
	"description": "Let's Raycast Presentation",
	"scripts": {
		"build": "svelte-kit sync && vite build",
		"preview": "svelte-kit sync && vite preview",
		"dev": "svelte-kit sync && vite dev",
		"prettier:fix": "prettier --write .",
		"prettier:check": "prettier --check .",
		"eslint:fix": "svelte-kit sync && eslint --fix .",
		"eslint:check": "svelte-kit sync && eslint --max-warnings 0 .",
		"typescript:check": "svelte-kit sync && tsc --noEmit",
		"svelte:check": "svelte-kit sync && svelte-check --fail-on-warnings",
		"build:check": "svelte-kit sync && svelte-check --fail-on-warnings --tsconfig ./tsconfig.json",
		"formatting:check": "prettier --check .",
		"formatting:fix": "prettier --write ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Rafal-Majewski/lets-raycast-presentation.git"
	},
	"author": "Rafa≈Ç Majewski",
	"license": "CC0-1.0",
	"bugs": {
		"url": "https://github.com/Rafal-Majewski/lets-raycast-presentation/issues"
	},
	"homepage": "https://github.com/Rafal-Majewski/lets-raycast-presentation#readme",
	"devDependencies": {
		"@sveltejs/adapter-node": "~1.3.1",
		"@sveltejs/kit": "~1.27.6",
		"@types/node": "~20.8.10",
		"@typescript-eslint/eslint-plugin": "~6.10.0",
		"@typescript-eslint/parser": "~6.10.0",
		"eslint": "~8.53.0",
		"eslint-plugin-import": "~2.29.0",
		"eslint-plugin-svelte": "~2.35.0",
		"espree": "~9.6.1",
		"globals": "~13.23.0",
		"prettier": "~3.0.3",
		"prettier-plugin-svelte": "~3.0.3",
		"sass": "~1.69.5",
		"svelte": "~4.2.2",
		"svelte-check": "~3.5.2",
		"svelte-eslint-parser": "~0.33.1",
		"svelte-preprocess": "~5.0.4",
		"ts-essentials": "~9.4.1",
		"typescript": "~5.2.2",
		"vite": "~4.5.0",
		"vitest": "~0.34.6"
	},
	"type": "module",
	"dependencies": {
		"dotenv": "~16.3.1",
		"envalid": "~8.0.0"
	}
}
